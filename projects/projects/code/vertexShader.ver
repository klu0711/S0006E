#version 430
layout(location=0) in vec3 pos;
layout(location=1) in vec4 color;
layout(location=2) in vec2 texCords;
layout(location=3) in vec3 normal;
out vec2 texCords2;
out vec3 uNormal;
out vec4 Color;
uniform mat4 transform;
void main()
{
	gl_Position = transform * vec4(pos, 1.0f);
	texCords2 = texCords;	
	Color = color;
	uNormal = normal;
}